<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EmployeeRegister</servlet-name>
  	<display-name>EmployeeRegister</display-name>
  	<description></description>
  	<servlet-class>main.servlet.EmployeeRegister</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeShow</servlet-name>
  	<display-name>EmployeeShow</display-name>
  	<description></description>
  	<servlet-class>main.servlet.EmployeeShow</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeUpdate</servlet-name>
  	<display-name>EmployeeUpdate</display-name>
  	<description></description>
  	<servlet-class>main.servlet.EmployeeUpdate</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeDelete</servlet-name>
  	<display-name>EmployeeDelete</display-name>
  	<description></description>
  	<servlet-class>main.servlet.EmployeeDelete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeDelete_</servlet-name>
  	<display-name>EmployeeDelete_</display-name>
  	<description></description>
  	<servlet-class>main.servlet.EmployeeDelete</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmployeeRegister</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeShow</servlet-name>
  	<url-pattern>/show</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeUpdate</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeDelete</servlet-name>
  	<url-pattern>/EmployeeDelete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeDelete_</servlet-name>
  	<url-pattern>/delete_</url-pattern>
  </servlet-mapping>
</web-app>
