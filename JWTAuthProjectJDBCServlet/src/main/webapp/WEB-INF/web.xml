<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<display-name>RegistrationServlet</display-name>
  	<description></description>
  	<servlet-class>main.servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>main.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreditSalary</servlet-name>
  	<display-name>CreditSalary</display-name>
  	<description></description>
  	<servlet-class>main.servlets.CreditSalary</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RolebasedAuthFilter</servlet-name>
  	<display-name>RolebasedAuthFilter</display-name>
  	<description></description>
  	<servlet-class>main.servlets.RolebasedAuthFilter</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminServlet</servlet-name>
  	<display-name>AdminServlet</display-name>
  	<description></description>
  	<servlet-class>main.servlets.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreditSalary</servlet-name>
  	<url-pattern>/credit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RolebasedAuthFilter</servlet-name>
  	<url-pattern>/filter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminServlet</servlet-name>
  	<url-pattern>/admin</url-pattern>
  </servlet-mapping>
</web-app>
